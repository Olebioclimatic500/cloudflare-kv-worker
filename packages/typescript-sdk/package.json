{
  "name": "@cloudflare-kv/typescript-sdk",
  "version": "0.1.0",
  "type": "module",
  "description": "TypeScript SDK for Cloudflare KV Worker API with dual authentication support (Bearer token for server-side, HMAC for browser)",
  "author": "Kulter Ryan",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/kulterryan/cloudflare-kv-worker.git",
    "directory": "packages/typescript-sdk"
  },
  "homepage": "https://github.com/kulterryan/cloudflare-kv-worker#readme",
  "bugs": {
    "url": "https://github.com/kulterryan/cloudflare-kv-worker/issues"
  },
  "keywords": [
    "cloudflare",
    "kv",
    "workers",
    "typescript",
    "sdk",
    "api",
    "hmac",
    "authentication",
    "key-value",
    "storage",
    "edge"
  ],
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "src",
    "README.md"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest",
    "lint": "tsc --noEmit",
    "prepublishOnly": "bun run build"
  },
  "devDependencies": {
    "tsup": "^8.0.1",
    "typescript": "^5.3.3",
    "vitest": "^1.2.0"
  },
  "peerDependencies": {
    "typescript": ">=4.5.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}